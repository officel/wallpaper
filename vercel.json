{
    "version": 2,
    "public": false,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET, OPTIONS",
                "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
            },
            "src": "/.*",
            "dest": "api/index.py"
        }
    ],
    "redirects": [
        {
            "source": "/",
            "destination": "https://github.com/officel/wallpaper"
        }
    ]
}
